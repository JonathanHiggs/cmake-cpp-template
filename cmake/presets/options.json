{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 27,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "option.enable-asserts",
      "hidden": true,
      "cacheVariables": {
        "MYLIB_ENABLE_ASSERTS": "ON"
      }
    },
    {
      "name": "option.enable-ipo",
      "hidden": true,
      "cacheVariables": {
        "MYLIB_ENABLE_IPO": "ON"
      }
    },
    {
      "name": "option.shared-libs",
      "hidden": true,
      "cacheVariables": {
        "MYLIB_ENABLE_SHARED": "ON"
      }
    },
    {
      "name": "option.warnings-as-errors",
      "hidden": true,
      "cacheVariables": {
        "MYLIB_ENABLE_WARNINGS_AS_ERRORS": "ON"
      }
    }
  ]
}
